@page
@model FUExchangeClient.Pages.Admin.DashboardModel
@{
}
<h1>Dashboard</h1>

<div class="row">
    <div class="col-md-6">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title">Number Product Sell</h5>
                <p class="card-text">@Model.DoneTransactionsCount</p>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title">Transactions Done</h5>
                <p class="card-text">@Model.DoneTransactionsCount</p>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title">Transactions Cancelled</h5>
                <p class="card-text">@Model.CancelledTransactionsCount</p>
            </div>
        </div>
    </div>
</div>
